pipeline {
    agent any

    stages {
       
        stage('Run Script') {
            steps {

                     sh 'chmod +x test.sh'  // Make script executable
                     sh './test.sh'
                  }
            }
           stage('sshconnect') {
              steps {
                  sshagent(credentials: ['PK_EC2'])
                      {
                         sh """
                           ssh ec2-user@3.236.242.10 'ls -a'
                          """
                       
                      }
                  

              }


           }



        }

}
